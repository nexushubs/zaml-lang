{
  "name": "@zaml/parser",
  "version": "0.5.2",
  "description": "ZAML tokenizer & lexer",
  "main": "lib/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf lib/*",
    "coverage": "nyc node_modules/.bin/mocha",
    "typings": "tsc --emitDeclarationOnly",
    "build": "babel src --out-dir lib --source-maps --extensions .ts && yarn typings",
    "watch": "babel src --out-dir lib --source-maps --extensions .ts --watch --verbose",
    "docs": "typedoc --out docs --theme markdown --readme none",
    "prepublishOnly": "yarn build && yarn docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nexushubs/zaml-lang/tree/master/packages/zaml-parser"
  },
  "author": "alphakevin <get.so.smart@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-unicode-property-regex": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/lodash": "^4.14.119",
    "@types/node": "^10.12.15",
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.2.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "typedoc": "^0.13.0",
    "typedoc-plugin-markdown": "^1.1.19",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "chalk": "^2.4.1",
    "lodash": "^4.17.11"
  },
  "gitHead": "be8ebbe2a12eca887a5d9696186bb42bc40beb29"
}
