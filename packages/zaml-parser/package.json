{
  "name": "@zaml/parser",
  "version": "0.4.0",
  "description": "ZAML tokenizer & lexer",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf lib",
    "coverage": "nyc node_modules/.bin/mocha",
    "build": "babel src --out-dir lib --source-maps",
    "watch": "babel src --out-dir lib --source-maps --watch --verbose",
